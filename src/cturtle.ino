  for(int i = 0; i < length; i++) {
    u8g2.clearBuffer(); // clear the internal memory
    u8g2.drawXBMP(uOffset, vOffset, width, height, mugbitmaps_rot[length-1-i]);
    u8g2.sendBuffer();
    //delay(10);
  }
}
