void setup(void)
{
  delay(1000);
  u8g2.begin();
  u8g2.setContrast(255); // set contrast to mauimum 
  u8g2.setBusClock(400000); //400kHz I2C 
  u8g2.setFont(u8g2_font_ncenB10_tr);
  t.u = uCenter;
  t.v = vCenter;
  t.ang = 0;
}

void loop(void)
{
  for(int i = 0; i < 70; i++) {
    u8g2.clearBuffer(); // clear the internal memory
    u8g2.drawXBMP(uOffset, vOffset, width, height, epd_bitmaps[i]);
    u8g2.sendBuffer();
    delay(10);
  }
}
